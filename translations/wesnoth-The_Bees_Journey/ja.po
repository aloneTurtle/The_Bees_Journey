#: The_Bees_Journey/scenarios/04_To_the_Voyage.cfg:27
msgid "Marie"
msgstr "Marie"

#. [unit]: type=Elder Bee, id=Noaille
#: The_Bees_Journey/scenarios/01_Wrathful_Morning.cfg:60
msgid "Noaille"
msgstr "Noaille"

#. [side]: type=Great Hornet, id=Veraly
#. [side]
#: The_Bees_Journey/scenarios/01_Wrathful_Morning.cfg:75
#: The_Bees_Journey/scenarios/01_Wrathful_Morning.cfg:169
